{
  "compileOnSave": false,
  "compilerOptions": {
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "target": "es5",
    "lib": [
      "es2017",
      "dom"
    ],
    "baseUrl": "src",
    "paths": {
      "@bn8-core/*":["app/core/*"],
      "@bn8-environments/*":["environments/*"],
      "@bn8-services/*":["app/services/*"],
      "@bn8-detalle/*":["app/navigator/detalle/*"],
      "@bn8-navigator/*":["app/navigator/*"],
      "@bn8-geoquery/*":["app/core/geofire/*"]
    }
  }
}
