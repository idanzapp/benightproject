{
  "compileOnSave": false,
  "compilerOptions": {
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "target": "es5",
    "lib": [
      "es2017",
      "dom"
    ],
    "baseUrl": "src",
    "paths": {
      "@bn8-core/*":["app/core/*"],
      "@bn8-imports/*":["app/core/imports/*"],
      "@bn8-interfaces/*":["app/core/interfaces/*"],
      "@bn8-constants/*":["app/core/constants/*"],
      "@bn8-directives/*":["app/directives/*"],
      "@bn8-environments/*":["environments/*"],
      "@bn8-services/*":["app/services/*"],
      "@bn8-detalle/*":["app/navigator/detalle/*"],
      "@bn8-login/*":["app/login/*"],
      "@bn8-preview/*":["app/modals/*"],
      "@bn8-navigator/*":["app/navigator/*"],
      "@bn8-database/*":["app/services/database/*"] 
    }
  }
}
